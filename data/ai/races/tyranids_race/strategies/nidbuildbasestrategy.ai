			self:DynamicBuild("tyranids_relic_hive", 2, 5, 2000, 0, 0, 0)
			self:DynamicBuild("tyranids_biotitan_hive", 1, 5, 2000, 0, 0, 0)
			self:DynamicBuild("tyranids_capillary_tower", 3, 5, 8000, 0, 0, 0)
			self:DynamicBuild("tyranids_capillary_tower", 8, 5, 16000, 0, 0, 0)
